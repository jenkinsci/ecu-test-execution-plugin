<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Package name="~de\.tracetronic\.cxs\.generated\.et\.client.*" />
    </Match>
    <Match>
        <Source name="~.*\.java" />
    </Match>
    <Match>
        <Class name="~.*_closure\d+" />
        <Bug pattern="SE_NO_SERIALVERSIONID" />
    </Match>
    <Match>
        <Field name="metaClass" />
        <Bug code="EI,EI2"/>
    </Match>
    <Match>
        <Class name="~.*_closure\d+" />
        <Bug code="EI2" />
    </Match>
    <Match>
        <Class name="~.*AbstractTestBuilder" />
        <Field name="context" />
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>
</FindBugsFilter>
