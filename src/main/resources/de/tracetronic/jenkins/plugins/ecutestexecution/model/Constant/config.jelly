<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="${%constant.label}" field="label">
        <f:textbox clazz="required"/>
    </f:entry>
    <f:entry title="${%constant.value}" field="value">
        <f:textbox/>
    </f:entry>
    <f:entry title="">
        <div align="right">
            <f:repeatableDeleteButton value="${%constant.delete}"/>
        </div>
    </f:entry>
</j:jelly>
